
N = int(input()) # 사람 수 
p_li = []
for i in range(N):
    kg, cm = input().split()
    p_li.append((int(kg), int(cm)))
cnt_li = []

for i in range(len(p_li)):
    cnt = 1
    kg, cm = p_li[i]
    for j in range(len(p_li)):
        if kg < p_li[j][0] and cm < p_li[j][1]:
            cnt+=1
    cnt_li.append(cnt)

print(*(cnt_li))
